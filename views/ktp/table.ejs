<table id="example1" class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>No</th>
        <th>NIK</th>
        <th>Nama</th>
        <th>Jenis Kelamin</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    <% for(let i = 0; i<ktp.length; i++){ %>
        <tr>
            <td><%= i+1 %></td>
            <td><%= ktp[i].nik %></td>
            <td><%= ktp[i].nama %></td>
            <td><%= ktp[i].jenis_kelamin %></td>
            <td>
                <a href="/penduduk/edit/<%= ktp[i].id %>" class="btn btn-info btn-sm">
                    <i class="fas fa-pen mr-1"></i> Edit
                </a>
            </td>
        </tr>
    <% } %>
    </tbody>
  </table>